<LoginScreen>:
    name:"Login Screen"
    LoginArea:
        MDCard:
            size_hint:None,None
            size:dp(370),dp(400)
            pos_hint:{"center_x":.5,"center_y":.5}
            elevation:5
            spacing: 25
            padding:25
            orientation:'vertical'


            MDLabel:
                text:"Login"
                font_size:25
                halign:'center'
                size_hint_y:None
                height:self.texture_size[1]


            TextInput:
                id:username

                size_hint:None,None
                width:"200dp"
                height:"35dp"
                font_size: 18
                hint_text:"Username"
                pos_hint: {"center_x": 0.5}
                multiline:False
                on_text_validate : root.reset()


            TextInput:

                id:password
                password:True

                size_hint:None,None
                width:"200dp"
                height:"35dp"
                font_size: 18
                hint_text:"Password"
                pos_hint: {"center_x": 0.5}
                multiline:False
                on_text_validate :root.reseto()



            Label:
                id:incorrect
                background_color:0,0,0,0




                Image:
                    source:"C:\\Users\\USER\\Pictures\\Game Sprites\\error.png"
                    center_x:self.parent.center_x
                    center_y:self.parent.center_y
                    size_hint_y:None
                    width:dp(350)
                    allow_stretch:True
                    #disabled:not self.parent.visible0

                    opacity:root.check

            Button:
                id:login
                text:"login"
                font_size:0
                background_color:0,0,0,0
                on_press:root.login()





                Image:
                    source:"C:\\Users\\USER\\Pictures\\Game Sprites\\login.png"
                    center_x:self.parent.center_x
                    center_y:self.parent.center_y
<HomeScreen>:
    name:"homescreen"
    FloatLayout:
        size:root.width,root.height

        MDCard:
            orientation:'vertical'
            pos_hint:{"center_x":0.5,"center_y":0.1}
            size_hint:None,None
            width:root.width-(root.width*1/6)
            height:root.height-(root.height*1/8)
            pos_hint:{"center_x":.5,"center_y":.5}
            elevation:5



            Label:


                font_size:30
                text:"Latest Items"

            Label:

                font_size:20
                text:"Found Items"



            HomeFound:
                size_hint:None,None
                width:self.parent.width
                height:self.parent.height/4
                #cols:12
                rows:2

                Button:
                    id:ipad
                    text:"Ipad"
                    on_press:root.itemdescription()
                Button:
                    on_press:root.sweater()
                    id:sweater
                    text:"sweater"
            Label:
                pos_hint:{"center_x":.2}
                font_size:20
                text:"Lost Items"



            HomeFound:
                size_hint:None,None
                width:self.parent.width
                height:self.parent.height/4
                rows:2

                Button:
                    text:"Ipad"

                Button:
                    text:"sweater"




        MDCard:
            id:menu
            orientation:'vertical'
            pos_hint:{"x":root.switch}
            size_hint:None,None
            width:root.width/4.5
            height:root.height

            MDCard:
                orientation:'vertical'
                padding:-20
                pos_hint:{"top":1}


                Label:
                    center_x:self.parent.center_x
                    font_size:20
                    text:"Menu"

            Spinner:
                id:FoundItemsCategories
                text:"Found Items"
                values:['Electronics','Stationary','Books','Clothes','Other']
                on_text:root.FoundCategories(FoundItemsCategories.text)
                #sync_height=True

            Label:
            Label:
            Label:
            Label:
            Label:


            Button:
                center_x:self.parent.center_x
                center_y:self.parent.center_y
                text:"Report Lost Items"
                on_press:root.reportitems()

            Label:
            Label:
            Label:
            Label:
            Label:




        Button:
            background_color:0,0,0,0
            pos_hint:{"center_y":0.5,"x":root.extend_x}
            size_hint:None,None
            width:40
            height:90


            on_press:root.extend()

            Image:
                opacity:1
                source:root.extendSign
                center_x:self.parent.center_x
                center_y:self.parent.center_y
        TopMenu:
            orientation:'horizontal'
            pos_hint:{"x":0.22, "top":1}
            size_hint:None,None
            width:root.width*0.77
            height:"35dp"

            MDCard:
                pos_hint:{"center_x":1, "top":1}

                width:"770dp"
                height:"35dp"

                TextInput:
                    id:searchbar
                    font_size:18

                    hint_text:"Search"
                    on_text_validate:root.search()

                Button:
                    pos_hint:{"x":.3, "top":.95}
                    size_hint:None,None
                    width:"30dp"
                    height:"35dp"
                    background_color:0,0,0,0


                    Image:
                        source:"C:\\Users\\USER\\Pictures\\Game Sprites\\searchicon.png"
                        width:"30dp"
                        height:"30dp"
                        center_x:self.parent.center_x
                        center_y:self.parent.center_y


                MDCard:
                    pos_hint:{"x":.754,"top":1}
                    size_hint:None,None
                    width:"270dp"
                    height:"35dp"

                    Label:
                        text:"Username"


                Button:
                    pos_hint:{"x":.965, "top":1}
                    size_hint:None,None
                    width:"27dp"
                    height:"30dp"
                    background_color:0,0,0,0
                    on_press:root.logout()
                    Image:
                        source:"C:\\Users\\USER\\Pictures\\Game Sprites\\logo.jpg"
                        center_x:self.parent.center_x
                        center_y:self.parent.center_y
                        width:"65dp"
                        height:"42dp"




